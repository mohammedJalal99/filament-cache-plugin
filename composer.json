{
  "name": "mohammedjalal99/filament-cache-plugin",
  "description": "Complete caching solution for Filament PHP - Cache everything automatically",
  "keywords": ["filament", "laravel", "cache", "performance", "plugin"],
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Mohammed Jalal", "email": "hamaj4095@gmail.com" }
  ],
  "require": {
    "php": "^8.1",
    "filament/filament": "^3.0",
    "illuminate/contracts": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "FilamentCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FilamentCache\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FilamentCache\\FilamentCacheServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  },
  "replace": {
    "mohammedJalal99/filament-cache-plugin": "self.version"
  },
  "conflict": {
    "filament/filament": "<3.0"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  }
}
